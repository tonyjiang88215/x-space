{
    "compilerOptions": {
        "outDir": "./dist",
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
        "module": "es6",
        "target": "es5",
        "allowJs": true,
        "jsx": "react",
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "nonde_modules"
    ],
    "plugins": [
        {
            "name": "typescript-workspace-plugin"
        }
    ]
}